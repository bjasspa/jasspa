0 define-macro svn-status-refresh
  !if .find-file.file-count
    set-alpha-mark "\x81"
    -1 buffer-mode "view"
    set-variable $window-line 5
    !while &not &seq @wc ""
      !if &seq @wc "*"
        set-variable @wc " "
        set-variable :tag-count &sub :tag-count 1
        !if &seq "X" &stat "t" &cat .find-file.file-dir &rig @wl :fmpf
          forward-char
          !if &seq @wc "?"
            backward-char
            set-mark
            forward-line
            -1 kill-region
            backward-line
          !else
            set-variable @wc "!"
            backward-char
          !endif
        !endif
      !endif
      forward-line
    !done
    -1 buffer-mode "edit"
    1 buffer-mode "view"
    goto-alpha-mark "\x81"
  !endif
!emacro

