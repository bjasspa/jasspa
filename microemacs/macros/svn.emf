0 define-macro svn-exec-complete7
  set-variable $buffer-hilight .hilight.diff
  end-of-buffer
  backward-line
  !if &seq &lef @wl 9 "[**** SVN"
    set-variable $line-scheme &con &sin "completed successfully" @wl .scheme.no1 .scheme.error
  !endif
