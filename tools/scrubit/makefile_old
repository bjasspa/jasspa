# Set up the defaults for the environment.
RM = rm -f
CC = gcc $(CFLAGS)
LD = gcc
CFLAGS	= -O3 -I. -DTSDUMP_PLATFORM_SUNOS
LDFLAGS = -O3

SRC	=	scrubit.c
OBJ	=	$(SRC:.c=.o)
TARGET	=	scrubit

all:	$(TARGET)

scrubit:	$(OBJ)
	$(LD) $(LDFLAGS) -o $@ $(OBJ) 

clean:	
	$(RM) ${TARGET} 
	$(RM) *.o

spotless: clean
	$(RM) *~
	$(RM) core
	$(RM) tags
			


